{
  "main": "dist/index.js",
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "bun-types": "^1.0.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx . && prettier --check .",
    "fix": "eslint --fix --ext .js,.jsx,.ts,.tsx . && prettier --write .",
    "dev": "bun --watch src/server/index.ts",
    "build": "bun build --minify --outdir dist src/server/index.ts",
    "start": "bun run ."
  },
  "type": "module"
}
